{
  "name": "medical-prescription-ocr",
  "version": "1.0.0",
  "description": "Express + TypeScript backend for OCRing prescription images and parsing medications via LLM.",
  "main": "dist/server.js",
  "scripts": {
    "dev": "nodemon --watch src --ext ts --exec ts-node src/server.ts",
    "build": "tsc",
    "start": "node dist/server.js",
    "lint": "eslint . --ext .ts || true"
  },
  "keywords": [
    "express",
    "typescript",
    "ocr",
    "google-vision",
    "openai",
    "medical"
  ],
  "license": "MIT",
  "dependencies": {
    "@google-cloud/vision": "^4.3.3",
    "axios": "^1.7.9",
    "cors": "^2.8.5",
    "dotenv": "^16.4.5",
    "express": "^4.19.2",
    "multer": "^1.4.5-lts.1",
    "openai": "^4.68.4"
  },
  "devDependencies": {
    "@types/cors": "^2.8.17",
    "@types/express": "^4.17.21",
    "@types/multer": "^1.4.12",
    "@types/node": "^22.7.4",
    "nodemon": "^3.1.5",
    "ts-node": "^10.9.2",
    "typescript": "^5.6.3"
  }
}
